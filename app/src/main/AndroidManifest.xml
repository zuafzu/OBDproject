<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cy.obdproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <application
        android:name=".app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".socket.WebSocketService" />
        <service android:name=".socket.SocketService" />

        <activity android:name=".activity.MainTestActivity" />
        <activity android:name=".activity.ReadBaseInfoActivity" />
        <activity android:name=".activity.WriteBaseInfoActivity" />
        <activity android:name=".activity.ErrorCodeActivity" />
        <activity android:name=".activity.DynamicDataActivity" />
        <activity android:name=".activity.IOTestActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.WriteDataActivity" />
        <activity android:name=".activity.DynamicData2Activity" />
        <activity android:name=".activity.IOTest2Activity" />
        <activity android:name=".activity.SelectRoleActivity" />
        <activity android:name=".activity.SelectCarTypeActivity" />
        <activity android:name=".activity.SelectSystemActivity" />
        <activity android:name=".activity.ConnentOBDActivity" />
        <activity android:name=".activity.RequestListActivity"></activity>
    </application>

</manifest>